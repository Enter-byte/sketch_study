void setup() {
  // put your setup code here, to run once:
  for (int j=0;j<8;j++){
    pinMode(lineNum[j],OUTPUT);
  }
}

void loop() {
  // put your main code here, to run repeatedly:
  for (int i=0;i<11;i++){
    for(int j=0;j<8;j++){
      digitalWrite(lineNum[j],leds[i][j]);
    }
    delay(300);
  }
}
